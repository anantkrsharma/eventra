{
    "version": 2,
    "builds": [
        {
            "src": "server.ts",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/server.ts"
        }
    ],
    "env": {
        "NODE_OPTIONS": "--no-warnings",
        "MCP_TRANSPORT": "network"
    },
    "functions": {
        "server.ts": {
            "maxDuration": 30
        }
    }
}
