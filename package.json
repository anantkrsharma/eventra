{
  "name": "eventra",
  "version": "1.0.0",
  "description": "",
  "type": "module",
  "scripts": {
    "start": "cross-env NODE_OPTIONS=--no-warnings ts-node --project tsconfig.node.json server.ts",
    "start:prod": "cross-env NODE_ENV=production NODE_OPTIONS=--no-warnings ts-node --project tsconfig.node.json server.ts",
    "start:network": "cross-env MCP_TRANSPORT=network NODE_OPTIONS=--no-warnings ts-node --project tsconfig.node.json server.ts",
    "start:network:prod": "cross-env NODE_ENV=production MCP_TRANSPORT=network NODE_OPTIONS=--no-warnings ts-node --project tsconfig.node.json server.ts",
    "build": "tsc",
    "serve": "cross-env NODE_OPTIONS=--no-warnings node dist/server.js",
    "serve:network": "cross-env MCP_TRANSPORT=network NODE_OPTIONS=--no-warnings node dist/server.js",
    "db:init": "cross-env NODE_OPTIONS=--no-warnings node init-db.js",
    "db:studio": "npx prisma studio",
    "db:migrate": "npx prisma migrate dev"
  },
